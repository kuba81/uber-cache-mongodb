{
  "author": "Paul Serby <paul@serby.net>",
  "name": "uber-cache-mongodb",
  "description": "Mongodb Engine for uber-cache",
  "version": "0.0.1",
  "tags": [
    "cache",
    "memory"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:serby/uber-cache-mongodb"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "main": "./mongodb-engine",
  "scripts": {
    "lint": "./node_modules/.bin/jshint . --reporter=./node_modules/jshint-full-path/index.js",
    "pretest": "npm run-script lint",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec -r should",
    "posttest": "./node_modules/.bin/istanbul check-coverage && rm -rf coverage",
    "prepublish": "npm test && npm prune"
  },
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "lodash": "^2.4.1"
  },
  "devDependencies": {
    "mocha": "1",
    "should": "3",
    "async": "^0.2.10",
    "uber-cache": "^1.0.0",
    "mongodb": "^1.3.23",
    "jshint-full-path": "^1.1.1",
    "istanbul": "^0.2.6"
  }
}
